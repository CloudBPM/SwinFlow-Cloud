<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context="com.xq.myxuanqi.ui.activity.RegistedActivity">
    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#fff"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:paddingTop="@dimen/dp_30"
        android:paddingBottom="@dimen/dp_10"
        android:id="@+id/tb">
        <ImageView
            android:layout_width="@dimen/dp_15"
            android:layout_height="@dimen/dp_15"
            android:background="@mipmap/arrow_left_000000"
            android:padding="@dimen/dp_10"
            android:gravity="center"
            android:id="@+id/tb_back"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_40"
            android:text="注册"
            android:textColor="#ff000000"
            android:textSize="@dimen/sp_16"
            android:gravity="center"/>
    </android.support.v7.widget.Toolbar>
    <android.support.constraint.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tb"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/cl"
        android:layout_marginTop="@dimen/dp_20">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="手&#8194;机&#8194;号"
            android:textSize="@dimen/sp_16"
            android:id="@+id/tv"
            app:layout_constraintTop_toTopOf="@id/et_tel"
            app:layout_constraintBottom_toBottomOf="@id/et_tel"/>
        <EditText
            android:layout_width="@dimen/dp_180"
            android:layout_height="@dimen/dp_30"
            app:layout_constraintLeft_toRightOf="@id/tv"
            android:layout_marginLeft="@dimen/dp_20"
            android:id="@+id/et_tel"
            android:paddingLeft="@dimen/dp_10"
            android:textSize="@dimen/sp_14"
            android:background="@drawable/btn_out_login"/>
    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/cl"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/cl1"
        android:layout_marginTop="@dimen/dp_20">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昵&#12288;&#12288;称"
            android:textSize="@dimen/sp_16"
            android:id="@+id/tv1"
            app:layout_constraintTop_toTopOf="@id/et_name"
            app:layout_constraintBottom_toBottomOf="@id/et_name"/>
        <EditText
            android:layout_width="@dimen/dp_180"
            android:layout_height="@dimen/dp_30"
            app:layout_constraintLeft_toRightOf="@id/tv1"
            android:layout_marginLeft="@dimen/dp_20"
            android:id="@+id/et_name"
            android:hint="选填"
            android:paddingLeft="@dimen/dp_10"
            android:textSize="@dimen/sp_14"
            android:background="@drawable/btn_out_login"/>
    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toBottomOf="@id/cl1"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginTop="@dimen/dp_20"
        android:id="@+id/cl2">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="所在单位"
        android:textSize="@dimen/sp_16"
        android:id="@+id/tv2"
        app:layout_constraintTop_toTopOf="@id/tv_company_name"
        app:layout_constraintBottom_toBottomOf="@id/tv_company_name"/>
    <TextView
        android:layout_width="@dimen/dp_180"
        android:layout_height="@dimen/dp_30"
        app:layout_constraintLeft_toRightOf="@id/tv2"
        android:layout_marginLeft="@dimen/dp_20"
        android:paddingLeft="@dimen/dp_10"
        android:textSize="@dimen/sp_14"
        android:gravity="center"
        android:text="无"
        android:id="@+id/tv_company_name"
        android:background="@drawable/btn_out_login"/>
</android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toBottomOf="@id/cl2"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/cl3"
    android:layout_marginTop="@dimen/dp_20">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="登录密码"
        android:textSize="@dimen/sp_16"
        android:id="@+id/tv3"
        app:layout_constraintTop_toTopOf="@id/et_password"
        app:layout_constraintBottom_toBottomOf="@id/et_password"/>
    <EditText
        android:layout_width="@dimen/dp_180"
        android:layout_height="@dimen/dp_30"
        app:layout_constraintLeft_toRightOf="@id/tv3"
        android:layout_marginLeft="@dimen/dp_20"
        android:paddingLeft="@dimen/dp_10"
        android:textSize="@dimen/sp_14"
        android:inputType="textPassword"
        android:id="@+id/et_password"
        android:background="@drawable/btn_out_login"/>
</android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toBottomOf="@id/cl3"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/cl4"
    android:layout_marginTop="@dimen/dp_20">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="验&#8194;证&#8194;码"
        android:textSize="@dimen/sp_16"
        android:id="@+id/tv4"
        app:layout_constraintTop_toTopOf="@id/et_code"
        app:layout_constraintBottom_toBottomOf="@id/et_code"/>
    <EditText
        android:layout_width="@dimen/dp_80"
        android:layout_height="@dimen/dp_30"
        app:layout_constraintLeft_toRightOf="@id/tv4"
        android:layout_marginLeft="@dimen/dp_20"
        android:id="@+id/et_code"
        android:paddingLeft="@dimen/dp_10"
        android:textSize="@dimen/sp_14"
        android:background="@drawable/btn_out_login"/>
        <TextView
            android:layout_width="@dimen/dp_80"
            android:layout_height="@dimen/dp_30"
            android:text="获取验证码"
            android:layout_marginLeft="@dimen/dp_20"
            android:gravity="center"
            android:id="@+id/tv_get_code"
            android:background="@drawable/btn_out_login"
            app:layout_constraintLeft_toRightOf="@id/et_code"/>
</android.support.constraint.ConstraintLayout>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_login_normal_bg"
        app:layout_constraintTop_toBottomOf="@id/cl4"
        android:layout_marginTop="@dimen/dp_30"
        android:layout_marginLeft="@dimen/dp_30"
        android:layout_marginRight="@dimen/dp_30"
        android:text="注册"
        android:id="@+id/btn_registed"
        android:textColor="#fff"/>
</android.support.constraint.ConstraintLayout>
